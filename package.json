{
  "name": "copilot-io",
  "version": "0.0.9",
  "description": "copilot-io",
  "license": "MIT",
  "author": "",
  "keywords": [
    "Alfred like",
    "Mutate",
    "laucher",
    "utility tools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OuyangQianba/copilot-io"
  },
  "files": [
    "lib",
    "screenshot",
    "copilot-javafx.jar"
  ],
  "bin": "lib/index.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage && rimraf .nyc_output && rimraf lib_test",
    "prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "prebuild": "npm run clean && npm run prettier && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "npm run clean && tsc -p tsconfig.test.json --pretty && nyc --exclude \"**/*-spec.js\" ava \"**/*-spec.js\" --verbose",
    "coverage": "nyc report --reporter=lcov --reporter=text --reporter=html",
    "server": "node lib/index.js",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "@types/socket.io": "^1.4.29",
    "b-logger": "^0.0.13",
    "cli-argparser": "^0.0.12",
    "copilot-core": "^0.0.32",
    "socket.io": "^2.0.3",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "@types/node": "^7.0.0",
    "ava": "^0.20.0",
    "coveralls": "^2.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
